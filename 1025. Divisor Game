Time: O(1)  Space: O(1)  Using observation

class Solution {
public:
    bool divisorGame(int n) {
        if(n&1)
            return 0;
        else 
            return 1;
    }
};

Time: O(n^2) Space: O(n)  using DP

class Solution {
public:
    bool divisorGame(int n) {
        vector<bool> dp(n+1);
        for(int i=2;i<=n;i++){
            for(int j=1;j<i;j++){
                if(i%j==0 and dp[i-j]==false)
                {dp[i]=true;break;}
            }
        }
        return dp[n];
    }
};

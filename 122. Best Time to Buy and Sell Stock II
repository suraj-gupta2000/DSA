Time: O(n)  Space: O(1)

class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int n=prices.size();
        int res=0;
        for(int i=1;i<n;i++){
            int temp=prices[i]-prices[i-1];
            if(temp>0)
                res+=temp;
        }
        return res;
    }
};

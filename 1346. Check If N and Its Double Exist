Time: O(n^2)  Space: O(1)

class Solution {
public:
    bool checkIfExist(vector<int>& arr) {
        int n=arr.size();
        for(int i=0;i<n;i++){
            int x=arr[i];
            for(int j=0;j<n;j++){
                if(arr[j]==2*arr[i] and i!=j)
                    return 1;
            }
        }
        return 0;
    }
};

Time: O(n log n)  Space: O(1)

class Solution {
public:
    bool checkIfExist(vector<int>& arr) {
        int n=arr.size();
        int c=0;
        for(int i=0;i<n;i++){
            if(arr[i]==0)
            {c++; arr[i]=10000;}
            if(c==2)
                return true;
        }
        sort(begin(arr),end(arr));
        for(int x: arr){
             if(binary_search(begin(arr),end(arr),2*x))
                 return true;
        }
        return false;
    }
};

Time : O(n*n)

class Solution {
public:
    int findTheDistanceValue(vector<int>& arr1, vector<int>& arr2, int d) {
        int n1=arr1.size(),n2=arr2.size();
        int res=n1;
        for(auto x:arr1){
            for(auto y:arr2){
                if(abs(x-y)<=d)
                {res--;break;}
            }
        }
        return res;
    }
};

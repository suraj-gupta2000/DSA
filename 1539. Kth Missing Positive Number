Time: O(n)  Space: O(1)

class Solution {
public:
    int findKthPositive(vector<int>& arr, int k) {
        for(auto x: arr){
            if(x<=k)
                k++;
            else
                break;
        }
        return k;
    }
};


Time: O(log n)  Space: O(1)

class Solution {
public:
    int findKthPositive(vector<int>& arr, int k) {
        int n=arr.size();
        int l=0,h=n-1;
        while(l<=h){
            int mid=l+(h-l)/2;
            if(arr[mid]-(mid+1)<k)
                l=mid+1;
            else
                h=mid-1;
        }
        return l+k;
    }
};

Time: O(n)

class Solution {
public:
    
    int getMaximumGenerated(int n) {
        if(n==0)
            return 0;
        vector<int> v(n+1,0);
        v[0]=0;v[1]=1;
        int i=1;
        while(2*i<=n and 2*i+1<=n){
            v[2*i]=v[i];
            v[2*i+1]=v[i]+v[i+1];
            i++;
        }
        int res= *max_element(v.begin(),v.end());
        return res;
    }
};

Time: O(n)  Space: O(1)

class Solution {
public:
    vector<int> twoSum(vector<int>& num, int t) {
        int n=num.size();
        int l=0,r=n-1;
        while(l<r){
            int sum=num[l]+num[r];
            if(sum==t) return {l+1,r+1};
            else if(sum<t)
                l++;
            else
                r--;
        }
        return {};
    }
};

Time: O(n)  Space: O(n)

class Solution {
public:
    int rob(vector<int>& nums) {
        int n=nums.size();
        vector<int> dp(n+1);
        dp[1]=nums[0];
        for(int i=2;i<=n;i++){
           dp[i]=max(dp[i-1],dp[i-2]+nums[i-1]);
        }
        return dp[n];
    }
};

Time: O(n)   Space: O(1)

class Solution {
public:
    int rob(vector<int>& nums) {
        int n=nums.size();
        if(n==1)
            return nums[0];
        
        int a=0,b=0,c=0;
        b=nums[0];
        for(int i=2;i<=n;i++){
            c=max(b,a+nums[i-1]);
            a=b;b=c;
        }
        return c;
    }
};

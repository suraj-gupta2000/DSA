Time: O(n log n)

class Solution {
public:
    int minimumSum(int num) {
        vector<int> v;
        while(num>0){
            v.push_back(num%10);
            num/=10;
        }
        sort(v.begin(),v.end());
        int x1=v[0],x2=v[1];
        x1=x1*10+v[2];
        x2=x2*10+v[3];
        return x1+x2;
    }
};

Time: O(n* Log n)  Space: O(1)

class Solution {
public:
    bool f(int x){
        int sum=0;
        while(x){
            sum+=x%10;
            x/=10;
        }
        return !(sum&1);
    }
    int countEven(int num) {
        if(num==1)
            return 0;
        int res=0;
        for(int i=2;i<=num;i++){
            if(f(i))
                res++;
        }
        return res;
    }
};

Time: O(Log n)  Space: O(1)

class Solution {
public:
    int countEven(int num) {
        int sum=0,temp=num;
        while(temp){
            sum+=temp%10;
            temp/=10;
        }
        int res= sum%2==0 ? num/2 : (num-1)/2;
        return res;
    }
};

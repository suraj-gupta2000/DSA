Time: O(n)  Space: O(n)

class Solution {
public:
    int mostFrequent(vector<int>& nums, int key) {
        int n=size(nums);
        map<int,int> m;
        for(int i=0;i<n-1;i++){
            if(nums[i]==key)
                m[nums[i+1]]++;
        }
        int res=0,temp=INT_MIN;
        for(auto& x: m)
            if(temp<x.second)
                res=x.first,temp=x.second;
        return res;
    }
};

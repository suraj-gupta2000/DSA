Time: O(Log minute)  Space: O(1)

class Solution {
public:
    int convertTime(string current, string correct) {
        int m1=10*(current[3]-'0')+(current[4]-'0'),
            m2=10*(correct[3]-'0')+(correct[4]-'0');
        int k=m2-m1;
        int h1=10*(current[0]-'0')+(current[1]-'0'),
            h2=10*(correct[0]-'0')+(correct[1]-'0');
        int k2=h2-h1;
        int tmin=k2*60+k;
        k=0;
        while(tmin){
            if(tmin>=60){
                k+=tmin/60;tmin%=60;
            }else if(tmin>=15)
                k+=tmin/15,tmin%=15;
            else if(tmin>=5)
                k+=tmin/5,tmin%=5;
            else
                k+=tmin/1,tmin%=1;
        }
        return k;
    }
};

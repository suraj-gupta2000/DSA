Time: O(n^2)  Space: O(1)
-->TLE

class Solution {
public:
    vector<int> countSmaller(vector<int>& nums) {
        int n=nums.size();
        vector<int> res(n);
        for(int i=0;i<n-1;i++){
            int k=0;
            for(int j=i+1;j<n;j++){
                if(nums[j]<nums[i])
                    k++;
                                  }
            res[i]=k;
        }
    return res;
    }
};

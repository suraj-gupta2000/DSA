Time: O(n*logn) Space: O(n)

class Solution {
public:
    int fun(int i){
        int ans=0;
        while(i){
            if(i&1){
                ans++;
            }
            i >>=1;
        }
        return ans;
    }
    vector<int> countBits(int n) {
        vector<int> res(n+1);
        for(int i=1;i<=n;i++){
            res[i]=fun(i);
        }
        return res;
    }
};

Time: O(n)  Space: O(n)

class Solution {
public:
    vector<int> countBits(int n) {
        vector<int> dp(n+1);
        dp[0]=0;
        for(int i=1;i<=n;i++){
            dp[i]=dp[i/2]+i%2;
        }
        return dp;
    }
};

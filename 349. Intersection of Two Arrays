Time: O(n logn)

class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
       sort(begin(nums1),end(nums1));
        sort(begin(nums2),end(nums2));
        int n1=nums1.size(),n2=nums2.size();
        set<int> res;
        for(int i=0,j=0;i<n1 and j<n2;){
            if(nums1[i]==nums2[j]){
                res.insert(nums1[i]);
                i++;j++;
            }
            else if(nums1[i]<nums2[j]){
                i++;
            }
            else{
                j++;
            }
        }
        vector<int> ans;
        for(int x:res)
            ans.push_back(x);
        return ans;
    }
};

Time: O(n*Log n)  Space: O(n)

class Solution {
public:
    int findContentChildren(vector<int>& g, vector<int>& s) {
        sort(begin(g),end(g));
        sort(begin(s),end(s));
        int res=0;
        int n=g.size(),m=s.size();
        if(m==0)
            return 0;
        for(int i=0,j=0;i<n and j<m;i++){
            if(g[i]<=s[j]){
                res++;j++;
            }
            else if(g[i]>s[j]){
                j++;i--;
            }
        }
        return res;
    }
};

Time: O(n*Log n)  Space: O(2*n)

class Solution {
public:
    vector<string> findRelativeRanks(vector<int>& s) {
        int n=size(s);
        if(n==1)
            return {"Gold Medal"};
        else if(n==2){
            if(s[0]>s[1])
                return {"Gold Medal","Silver Medal"};
            return {"Silver Medal","Gold Medal"};
        }
        vector<pair<int,int>> v;
        for(int i=0;i<n;i++){
            v.push_back(pair<int,int> {s[i],i});
        }
        sort(begin(v),end(v));
        
        vector<string> res(n);
        res[v[n-1].second]="Gold Medal";
        res[v[n-2].second]="Silver Medal";
        res[v[n-3].second]="Bronze Medal";
        for(int i=n-4,j=4;i>=0;i--,j++){
            int t=v[i].second;
            res[t]=to_string(j);
        }
        return res;
    }
};

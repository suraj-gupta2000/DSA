Time: O(n)  Space: O(1)

class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int res,maxend;
        res=maxend=nums[0];
        for(int i=1;i<nums.size();i++){
            maxend=max(maxend+nums[i],nums[i]);
            res=max(res,maxend);
        }
        return res;
    }
};

Time: O(nlogn)

class Solution {
public:
    int mySqrt(int x) {
        long int l=1,r=x,res=0;
        while(l<=r){
           long int mid=(l+r)/2;
            if(mid*mid==x)
                return mid;
            else if(mid*mid<x){
                res=mid;
                l=mid+1;
            }
            else
                r=mid-1;
        }
        return res;
    }
};

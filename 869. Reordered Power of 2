class Solution {
public:
    int fun(string s){
        int t=0;
        for(char c: s){
            t=t*10+  (c-'0');
        }
        return t;
    }
    bool permute(string s,int i=0){
        if(i==size(s)-1){
            if(s[0]=='0')
                return false;
            int t=fun(s);
            if(t & (t-1))
                return false;
            return true;
        }
        for(int j=i;j<size(s);j++){
            swap(s[i],s[j]);
            bool f=permute(s,i+1);
            if(f)
                return true;
            else
                swap(s[i],s[j]);
        }
        return false;
    }
    bool reorderedPowerOf2(int n) {
        string str=to_string(n);
        bool ans=permute(str);
        return ans;
    }
};

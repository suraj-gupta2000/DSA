Time: O(N1+N2)  Space: O(N2)

class Solution {
public:
    vector<int> countf(string& s){
        vector<int> v(26);
        for(auto& x: s){
            v[x-'a']++;
        }
        return v;
    }
    
    vector<string> wordSubsets(vector<string>& w1, vector<string>& w2) {
        int n1=size(w1),n2=size(w2);
        vector<string> res;
        vector<int> mf(26);
        for(auto& x: w2){
            vector<int> f = countf(x);
            for(int i=0;i<26;i++){
                mf[i]=max(mf[i],f[i]);
            }
        }
        for(auto& x: w1){
            vector<int> f= countf(x);
            int i=0;
            for( ; i<26;i++){
                if(f[i]<mf[i])
                    break;
            }
            if(i==26)
                res.push_back(x);
        }
        return res;
    }
};

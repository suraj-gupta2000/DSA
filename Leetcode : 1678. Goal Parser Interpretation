CPP solution :: Time: O(size(c))  Space: O(1)

class Solution {
public:
    string interpret(string c) {
        int n=size(c),i=0;
        string res="";
        while(i<n){
            if(c[i]=='G')
            {res+='G';i++;}
            else if(c[i]=='(' and c[i+1]==')')
            {   res+='o';i=i+2;}
            else{
                res+="al";
                i=i+4;
            }
        }
        return res;
    }
};

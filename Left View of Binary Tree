Time: O(n)

vector<int> leftView(Node *root)
{
    if(!root)
    return {};
   vector<int> res;
   queue<Node*> q;
   q.push(root);
   while(!q.empty()){
       int count=q.size();
       for(int i=0;i<count;i++){
          Node* curr=q.front();q.pop();
          if(i==0){
              res.push_back(curr->data);
          }
       if(curr->left)
       q.push(curr->left);
       if(curr->right)
       q.push(curr->right); 
       }
       
   }
   return res;
}

--> method 1

vector<string> AllPossibleStrings(string s){
		    // Code here
		    int n=s.size();
		    vector<string> res;
		    for(int i=1;i<(1<<n);i++){
		        string temp="";
		        for(int j=0;j<n;j++){
		            if(i &(1<<j)){
		                temp+=s[j];
		            }
		        }
		        res.push_back(temp);
		    }
		    sort(begin(res),end(res));
		    return res;
		}
